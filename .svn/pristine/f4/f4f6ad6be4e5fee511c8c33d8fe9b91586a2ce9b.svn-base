package org.loan.core.domain;

import java.io.Serializable;
import java.util.Date;

/**
 * 首页产品展示
 * 
 * @author hxy
 * @email huangxianyuan@gmail.com
 * @date 2018-07-20 14:48:42
 */
public class ClProductEntity implements Serializable {
    private static final long serialVersionUID = 1L;

    //主键
    private Long              id;
    //产品名称
    private String            name;
    //logo
    private String            logo;
    //描述
    private String            mark;
    //开启状态,1:启用，0：暂停
    private String            state;
    //排序
    private Integer           sort;
    //产品类型  详见borrowCatEnums
    private String            mortgageType;
    //创建时间
    private Date              createTime;
    //修改时间
    private Date              modifyTime;
    //最高可借额度
    private String            highCredit;
    //认证描述
    private String            authDesc;

    /**
     * 设置：主键
     */
    public void setId(Long id) {
        this.id = id;
    }

    /**
     * 获取：主键
     */
    public Long getId() {
        return id;
    }

    /**
     * 设置：产品名称
     */
    public void setName(String name) {
        this.name = name;
    }

    /**
     * 获取：产品名称
     */
    public String getName() {
        return name;
    }

    /**
     * Getter method for property <tt>logo</tt>.
     * 
     * @return property value of logo
     */
    public String getLogo() {
        return logo;
    }

    /**
     * Setter method for property <tt>logo</tt>.
     * 
     * @param logo value to be assigned to property logo
     */
    public void setLogo(String logo) {
        this.logo = logo;
    }

    /**
     * Getter method for property <tt>mark</tt>.
     * 
     * @return property value of mark
     */
    public String getMark() {
        return mark;
    }

    /**
     * Setter method for property <tt>mark</tt>.
     * 
     * @param mark value to be assigned to property mark
     */
    public void setMark(String mark) {
        this.mark = mark;
    }

    /**
     * 设置：开启状态,1:启用，0：暂停
     */
    public void setState(String state) {
        this.state = state;
    }

    /**
     * 获取：开启状态,1:启用，0：暂停
     */
    public String getState() {
        return state;
    }

    /**
     * 设置：排序
     */
    public void setSort(Integer sort) {
        this.sort = sort;
    }

    /**
     * 获取：排序
     */
    public Integer getSort() {
        return sort;
    }

    /**
     * 设置：产品类型  详见borrowCatEnums
     */
    public void setMortgageType(String mortgageType) {
        this.mortgageType = mortgageType;
    }

    /**
     * 获取：产品类型  详见borrowCatEnums
     */
    public String getMortgageType() {
        return mortgageType;
    }

    /**
     * 设置：创建时间
     */
    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    /**
     * 获取：创建时间
     */
    public Date getCreateTime() {
        return createTime;
    }

    /**
     * 设置：修改时间
     */
    public void setModifyTime(Date modifyTime) {
        this.modifyTime = modifyTime;
    }

    /**
     * 获取：修改时间
     */
    public Date getModifyTime() {
        return modifyTime;
    }

    /**
     * 设置：最高可借额度
     */
    public void setHighCredit(String highCredit) {
        this.highCredit = highCredit;
    }

    /**
     * 获取：最高可借额度
     */
    public String getHighCredit() {
        return highCredit;
    }

    /**
     * 设置：认证描述
     */
    public void setAuthDesc(String authDesc) {
        this.authDesc = authDesc;
    }

    /**
     * 获取：认证描述
     */
    public String getAuthDesc() {
        return authDesc;
    }
